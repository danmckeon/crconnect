<%= render 'partials/navbar' %>
<div class="pending-trials container">
  <div class="row">
    <div class="col-sm-1"></div>
    <div class="col-sm-10">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="pending-header">Pending Trials</h3>
        </div>
        <div class="panel-body">
          <table class="table table-hover table-bordered table-responsive">
            <tbody>
              <% @pending_trials.each do |trial| %>
                <tr>
                  <td><a href="#"> <%= trial.brief_title %> </a></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="col-sm-1"></div>
  </div>
</div>
